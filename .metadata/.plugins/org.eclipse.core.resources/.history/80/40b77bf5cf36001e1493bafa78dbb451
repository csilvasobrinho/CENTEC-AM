package listaExercicios02;

public class Questao04 {
	public static void main(String[] args) {
		int cont = 1, soma = 0;
		
		while(cont <= 10) {
			System.out.println("Contador em: " + cont);
			soma = soma + cont;
			cont++;
		}
		System.out.println("Somatório dos números de 1 a 10: " + soma);
	}

}
