package listaExercicios02;

import java.util.Scanner;

public class Questao07 {
	public static void main(String[] args) {
		int cont=1;
		double num=0;
		
		Scanner lerNum = new Scanner(System.in);
		
		while(cont <= 10) {
			System.out.println("Numero: ");
			num = lerNum.nextDouble();
			cont++;
		}
	}

}
